{
  "name": "@real-system/a11y-library",
  "version": "0.0.19",
  "description": "React aria library for real system.",
  "status": "production",
  "main": "src/index.ts",
  "sideEffects": false,
  "scripts": {
    "build": "NODE_ENV=production node ./tools/build.js && yarn types",
    "dev": "concurrently 'NODE_ENV=development node ./tools/dev.js' 'yarn types:watch'",
    "types:watch": "tsc --watch",
    "types": "tsc",
    "prepack": "clean-package -c ../../../tools/build/clean-package.config.json",
    "postpack": "clean-package r -c ../../../tools/build/clean-package.config.json",
    "tscheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigwoof91/real-system.git"
  },
  "keywords": [
    "real-system",
    "react-aria"
  ],
  "author": "Mike Wolf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigwoof91/real-system/issues"
  },
  "homepage": "https://github.com/bigwoof91/real-system#readme",
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "concurrently": "^8.2.2",
    "react": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@react-aria/checkbox": "^3.11.2",
    "@react-aria/focus": "^3.14.3",
    "@react-aria/interactions": "^3.19.1",
    "@react-aria/radio": "^3.8.2",
    "@react-aria/utils": "3.22.0",
    "@react-stately/checkbox": "^3.5.1",
    "@react-stately/radio": "^3.9.1",
    "@react-stately/toggle": "^3.6.3",
    "@react-types/checkbox": "^3.5.2",
    "@react-types/radio": "^3.5.2",
    "@react-types/shared": "^3.21.0"
  },
  "files": [
    "lib"
  ],
  "main:test": "src/index.ts"
}

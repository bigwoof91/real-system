{
  "name": "@real-system/stack",
  "version": "0.0.18",
  "description": "Stack component that distributes layouts for real system.",
  "status": "production",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "sideEffects": false,
  "scripts": {
    "build": "NODE_ENV=production node ./tools/build.js && yarn types",
    "build:js": "NODE_ENV=production node ./tools/build.js",
    "dev": "concurrently 'NODE_ENV=development node ./tools/dev.js' 'yarn types:watch'",
    "types:watch": "tsc --watch",
    "types": "tsc",
    "prepack": "clean-package -c ../../../tools/build/clean-package.config.json",
    "postpack": "clean-package r -c ../../../tools/build/clean-package.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigwoof91/real-system.git"
  },
  "keywords": [
    "real-system",
    "stack"
  ],
  "author": "Mike Wolf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigwoof91/real-system/issues"
  },
  "homepage": "https://github.com/bigwoof91/real-system#readme",
  "peerDependencies": {
    "@real-system/elements-primitive": "^0.0.22",
    "@real-system/styled-library": "^0.0.20",
    "@real-system/utils-library": "^0.0.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@real-system/elements-primitive": "^0.0.22",
    "@real-system/styled-library": "^0.0.20",
    "@real-system/utils-library": "^0.0.32",
    "clean-package": "^2.2.0",
    "concurrently": "^8.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "files": [
    "lib"
  ],
  "main:test": "src/index.ts"
}

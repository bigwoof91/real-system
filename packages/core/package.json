{
  "name": "@real-system/core",
  "version": "0.0.27",
  "status": "production",
  "description": "Contains all the packages from Real System.",
  "author": "Michael Wolf",
  "license": "MIT",
  "main:dev": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:js && yarn build:types",
    "build:js": "yarn clean && yarn update-dependencies && yarn generate-bundle && yarn compile",
    "build:types": "./tools/build-types.sh",
    "clean": "./tools/clean-packages.sh",
    "compile": "babel ./src/*.ts --out-dir ./lib --env-name 'release' --extensions '.ts' --config-file './babel.config.js' && yarn minify-bundle",
    "update-dependencies": "node ./tools/updateDependencies.js",
    "generate-bundle": "node ./tools/generate.js",
    "minify-bundle": "node ./tools/minify"
  },
  "dependencies": {
    "@real-system/a11y-library": "^0.0.0",
    "@real-system/alert": "^0.0.9",
    "@real-system/alert-primitive": "^0.0.5",
    "@real-system/animation-library": "^0.0.7",
    "@real-system/box-primitive": "^0.0.8",
    "@real-system/button": "^0.0.25",
    "@real-system/button-primitive": "^0.0.18",
    "@real-system/checkbox": "^0.0.10",
    "@real-system/dialog": "^0.0.17",
    "@real-system/dialog-primitive": "^0.0.13",
    "@real-system/field": "^0.0.2",
    "@real-system/flex": "^0.0.17",
    "@real-system/icon": "^0.0.24",
    "@real-system/input": "^0.0.23",
    "@real-system/portal-primitive": "^0.0.5",
    "@real-system/radio-group": "^0.0.3",
    "@real-system/spinner": "^0.0.23",
    "@real-system/state-library": "^0.0.6",
    "@real-system/styling-library": "^0.0.6",
    "@real-system/text-primitive": "^0.0.6",
    "@real-system/textarea": "^0.0.7",
    "@real-system/theme-library": "^0.0.9",
    "@real-system/toaster": "^0.0.8",
    "@real-system/tooltip": "^0.0.1",
    "@real-system/truncate": "^0.0.2",
    "@real-system/types-library": "^0.0.5",
    "@real-system/typography": "^0.0.24",
    "@real-system/utils-library": "^0.0.9",
    "@real-system/visually-hidden": "^0.0.21"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.5"
  }
}

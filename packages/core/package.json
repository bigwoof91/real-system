{
  "name": "@realsystem/core",
  "version": "1.7.0",
  "status": "production",
  "description": "Contains all the packages from Real System.",
  "author": "Michael Wolf",
  "license": "MIT",
  "main:dev": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean && yarn generate-bundle && yarn compile && yarn build:types",
    "build:types": "./tools/build-types.sh",
    "clean": "./tools/clean-packages.sh",
    "compile": "babel ./src/*.ts --out-dir ./lib --extensions '.ts' && node ./tools/minify",
    "compile:dev": "babel ./src/*.ts --out-dir ./lib --extensions '.ts'",
    "generate-bundle": "node ./tools/generate.js",
    "minify-bundle": "node ./tools/minify"
  },
  "dependencies": {
    "@realsystem/box": "^8.0.0",
    "@realsystem/button": "^8.0.0",
    "@realsystem/styling": "^1.6.0",
    "@realsystem/theme": "^2.0.0",
    "@realsystem/utils": "^1.5.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}

{
  "name": "@realsystem/core",
  "version": "1.4.2",
  "status": "production",
  "description": "Contains all the packages from Real System.",
  "author": "Michael Wolf",
  "license": "MIT",
  "main:dev": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn generate-bundle && yarn compile && yarn build:types",
    "build:types": "./tools/build-types.sh",
    "clean": "./tools/clean-packages.sh",
    "compile": "babel ./src/*.ts --out-dir ./lib --extensions '.ts' && yarn minify-bundle",
    "compile:dev": "babel ./src/*.ts --out-dir ./lib --extensions '.ts'",
    "generate-bundle": "node ./tools/generate.js",
    "minify-bundle": "node ./tools/minify"
  },
  "dependencies": {
    "@realsystem/box": "^6.0.2",
    "@realsystem/button": "^6.0.2",
    "@realsystem/styling": "^1.4.2",
    "@realsystem/utils": "^1.4.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}

    "@real-system/typography": "^0.0.40",
    "@real-system/utils-library": "^0.0.19",
    "@real-system/visually-hidden": "^0.0.36"
  },
  "peerDependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"

{
  "name": "@real-system/core",
  "version": "0.0.2",
  "status": "production",
  "description": "Contains all the packages from Real System.",
  "author": "Michael Wolf",
  "license": "MIT",
  "main:dev": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prebuild": "yarn clean && yarn update-dependencies && yarn generate-bundle",
    "build": "yarn clean && yarn update-dependencies && yarn generate-bundle && yarn compile && yarn build:types",
    "build:types": "./tools/build-types.sh",
    "clean": "./tools/clean-packages.sh",
    "compile": "babel ./src/*.ts --out-dir ./lib --extensions '.ts' --config-file './babel.config.js' && node ./tools/minify",
    "compile:dev": "babel ./src/*.ts --out-dir ./lib --extensions '.ts'",
    "update-dependencies": "node ./tools/updateDependencies.js",
    "generate-bundle": "node ./tools/generate.js",
    "minify-bundle": "node ./tools/minify"
  },
  "dependencies": {
    "@real-system/box": "^0.0.1",
    "@real-system/button": "^0.0.2",
    "@real-system/icon": "^0.0.1",
    "@real-system/styling": "^0.0.0",
    "@real-system/theme": "^0.0.0",
    "@real-system/typography": "^0.0.1",
    "@real-system/utils": "^0.0.0",
    "@real-system/visually-hidden": "^0.0.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.2"
  }
}

{
  "name": "real-system",
  "private": true,
  "version": "0.0.0",
  "description": "A real react component system that is easily themeable and extendable.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigwoof91/real-system.git"
  },
  "keywords": [
    "real system",
    "design system",
    "component library",
    "react",
    "styled-system",
    "emotion",
    "theme"
  ],
  "author": "Michael Wolf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigwoof91/real-system/issues"
  },
  "homepage": "https://github.com/bigwoof91/real-system#readme",
  "workspaces": [
    "packages/*",
    "packages/components/*",
    "packages/libraries/*",
    "packages/primitives/*"
  ],
  "scripts": {
    "___________QUALITY___________": "code quality and maintenance",
    "lint": "eslint \"packages/**/*.{ts,tsx,mdx}\"",
    "lint:fix": "yarn lint --fix",
    "typecheck": "yarn workspaces foreach --topological-dev run types --noEmit --emitDeclarationOnly false",
    "precise-commits": "precise-commits",
    "pkgs:check": "manypkg check",
    "pkgs:fix": "manypkg fix",
    "commit": "git cz",
    "update-dependabot-config": "node ./tools/update-dependabot-config.js",
    "___________TESTING___________": "test scripts",
    "pretest": "yarn workspace @real-system/core clean && node ./.jest/preTest.js",
    "test": "yarn pretest && jest ./packages/",
    "test:coverage": "jest ./packages/ --runInBand --coverage",
    "___________RELEASE___________": "",
    "changeset": "changeset",
    "version:stable": "yarn changeset version && yarn install --mode=update-lockfile",
    "prerelease": "yarn lint && yarn test --runInBand",
    "release:stable": "yarn build && yarn changeset publish",
    "___________BUILD___________": "build scripts",
    "productionize": "node tools/productionize-package.js",
    "build": "yarn workspaces foreach --topological-dev run build",
    "build:js": "yarn workspaces foreach --topological-dev run build:js",
    "build:core": "yarn workspace @real-system/core run build",
    "build:pkg": "node tools/build-package.js",
    "build:storybook": "yarn build && storybook build -o _storybook/dist -c _storybook",
    "___________DEVELOPMENT___________": "dev scripts",
    "bootstrap": "yarn && husky install",
    "dev": "node ./tools/run-workspace-dev --select=true --with=yarn:storybook",
    "storybook": "storybook dev -c _storybook -p 6006",
    "cook:pkg": "node ./tools/generate-package"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@commitlint/cz-commitlint": "^17.4.2",
    "@commitlint/prompt-cli": "^17.4.2",
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@emotion/jest": "^11.10.5",
    "@manypkg/cli": "^0.19.2",
    "@storybook/addon-a11y": "=7.0.0-beta.21",
    "@storybook/addon-actions": "=7.0.0-beta.21",
    "@storybook/addon-essentials": "=7.0.0-beta.21",
    "@storybook/addon-interactions": "=7.0.0-beta.21",
    "@storybook/addon-links": "=7.0.0-beta.21",
    "@storybook/addon-viewport": "=7.0.0-beta.21",
    "@storybook/builder-vite": "=7.0.0-beta.21",
    "@storybook/channel-websocket": "=7.0.0-beta.21",
    "@storybook/client-api": "=7.0.22",
    "@storybook/jest": "^0.0.11-next.0",
    "@storybook/preview-web": "=7.0.0-beta.21",
    "@storybook/react": "=7.0.0-beta.21",
    "@storybook/react-vite": "=7.0.0-beta.21",
    "@testing-library/dom": "^8.19.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",

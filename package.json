    "minimist": "^1.2.7",
    "precise-commits": "^1.0.2",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.8",
    "storybook": "=7.0.0-beta.21",
    "terser": "^5.16.1",

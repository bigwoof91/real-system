{
  "name": "realsystem",
  "private": true,
  "version": "0.0.0",
  "description": "A real component system for responsive web applications that is themeable and extendable.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigwoof91/realsystem.git"
  },
  "keywords": [
    "design system",
    "component library",
    "react",
    "styled-system",
    "styled-components",
    "theme"
  ],
  "author": "Michael Wolf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigwoof91/realsystem/issues"
  },
  "homepage": "https://github.com/bigwoof91/realsystem#readme",
  "workspaces": [
    "packages/*",
    "packages/libraries/*"
  ],
  "main": "index.js",
  "module": "index.esm.js",
  "types": "index.d.ts",
  "scripts": {
    "__QUALITY__": "code quality and maintenance",
    "lint": "eslint packages/**/*.{ts,tsx,mdx}",
    "lint:fix": "yarn lint -- --fix",
    "precise-commits": "precise-commits",
    "packages:check": "manypkg check",
    "packages:fix": "manypkg fix",
    "commit": "git-cz",
    "__RELEASE": "",
    "pkg:changeset": "changeset",
    "pkg:version": "changeset version",
    "pkg:release:": "yarn changeset publish",
    "__BUILD__": "build scripts",
    "prebuild": "run-s packages:check",
    "build": "run-s build:styling build:theme build:utils build:box build:icon build:typography build:button build:core",
    "build:button": "yarn workspace @realsystem/button build",
    "build:box": "yarn workspace @realsystem/box build",
    "build:core": "yarn workspace @realsystem/core build",
    "build:icon": "yarn workspace @realsystem/icon build",
    "build:styling": "yarn workspace @realsystem/styling build",
    "build:theme": "yarn workspace @realsystem/theme build",
    "build:typography": "yarn workspace @realsystem/typography build",
    "build:utils": "yarn workspace @realsystem/utils build",
    "build:storybook": "build-storybook",
    "__DEVELOPMENT__": "dev scripts",
    "bootstrap": "yarn && husky install",
    "dev": "concurrently 'yarn:dev:*' 'yarn storybook'",
    "dev:button": "yarn workspace @realsystem/button dev",
    "dev:box": "yarn workspace @realsystem/box dev",
    "dev:icon": "yarn workspace @realsystem/icon dev",
    "dev:styling": "yarn workspace @realsystem/styling dev",
    "dev:theme": "yarn workspace @realsystem/theme dev",
    "dev:typography": "yarn workspace @realsystem/typography dev",
    "dev:utils": "yarn workspace @realsystem/utils dev",
    "__SANDBOX__": "local sandbox & website scripts",
    "website": "cd website && yarn start",
    "storybook": "start-storybook -p 6006"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@changesets/changelog-github": "^0.4.1",
    "@changesets/cli": "^2.17.0",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@commitlint/cz-commitlint": "^13.2.1",
    "@commitlint/prompt-cli": "^13.2.1",
    "@expo/spawn-async": "^1.5.0",
    "@manypkg/cli": "^0.18.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-viewport": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@testing-library/dom": "^8.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-jest": "^27.2.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "commitizen": "^4.2.4",
    "concurrently": "^6.2.1",
    "csstype": "^3.0.9",
    "directory-named-webpack-plugin": "^4.0.1",
    "esbuild": "^0.12.25",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-mdx": "^1.15.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^4.12.4",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "global": "^4.4.0",
    "husky": "^7.0.2",
    "jest": "^27.2.3",
    "lerna": "^4.0.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "precise-commits": "^1.0.2",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-esbuild": "^4.6.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "semantic-release": "^18.0.0",
    "semantic-release-monorepo": "^7.0.5",
    "storybook-addon-performance": "^0.16.1",
    "styled-components": "^5.3.3",
    "terser": "^5.9.0",
    "type-fest": "^2.5.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  }
}

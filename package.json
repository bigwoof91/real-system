{
  "name": "realsystem",
  "version": "1.0.0",
  "description": "A real component system for responsive web applications that is themeable and extendable.",
  "workspaces": [
    "packages/*"
  ],
  "main": "index.js",
  "module": "index.esm.js",
  "types": "index.d.ts",
  "scripts": {
    "__BUILD__": "build scripts",
    "build": "run-s build:primitive build:button",
    "build:button": "npm run build --workspace=@realsystem/button",
    "build:primitive": "npm run build --workspace=@realsystem/primitive",
    "__DEVELOPMENT__": "dev scripts",
    "preinstall": "npx npm-force-resolutions",
    "bootstrap": "npm run build",
    "dev": "concurrently npm:primitive npm:button  npm:playroom",
    "button": "npm run dev -w @realsystem/button",
    "primitive": "npm run dev -w @realsystem/primitive",
    "__SANDBOX__": "local sandbox & website scripts",
    "playroom": "npm start -w @realsystem/playroom",
    "website": "cd website && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigwoof91/realsystem.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigwoof91/realsystem/issues"
  },
  "homepage": "https://github.com/bigwoof91/realsystem#readme",
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@emotion/babel-plugin": "^11.3.0",
    "@testing-library/dom": "^8.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-jest": "^27.1.1",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "concurrently": "^6.2.1",
    "esbuild": "^0.12.25",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "execa": "^5.1.1",
    "jest": "27.1.1",
    "npm-dts": "^1.3.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "prettier": "^2.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.4.2"
  },
  "resolutions": {
    "react": "17.0.2"
  }
}
